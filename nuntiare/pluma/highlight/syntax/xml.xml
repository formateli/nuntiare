<highlightDefinition caseSensitive="false"  extensions="xml,build,page,rdl,rdlc,addin,nuntiare">

	<styles>
		<style name="reserved" foreColor="green" backColor="" bold="false" italic="false" />
		<style name="comment" foreColor="blue" backColor="" bold="false" italic="false" />
		<style name="quote" foreColor="red" backColor="" bold="false" italic="false" />
	</styles>

	<separators>

	</separators>

	<descriptors>

		<descriptor style="reserved" type="Contains" description="" >
			<tokens>
				<token value="&lt;"/>
				<token value="&gt;"/>
				<token value="&lt;/"/>
				<token value="/&gt;"/>
			</tokens>
		</descriptor>

		<descriptor style="quote" type="ToCloseToken" description="Close string between quotes">
			<tokens>
				<token value="&quot;" closeToken="&quot;"/>
			</tokens>
		</descriptor>

		<descriptor style="comment" type="ToCloseToken" multiLine="True" description="Comment block">
			<tokens>
				<token value="&lt;!--" closeToken="--&gt;" />
			</tokens>
		</descriptor>

	</descriptors>

</highlightDefinition>
