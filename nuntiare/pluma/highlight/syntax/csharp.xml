<highlightDefinition caseSensitive="true" extensions="cs">

	<styles>
		<style name="reserved" foreColor="blue" backColor="" bold="false" italic="false" />
		<style name="comment" foreColor="green" backColor="" bold="false" italic="false" />
		<style name="region" foreColor="255,0,0" backColor="" bold="false" italic="false" />
		<style name="quote" foreColor="red" backColor="" bold="false" italic="false" />
	</styles>

	<separators>
		<separator value="." />
		<separator value="," />
		<separator value="-" />
		<separator value="+" />
		<separator value="=" />
		<separator value="(" />
		<separator value=")" />
		<separator value="[" />
		<separator value="]" />
		<separator value="{" />
		<separator value="}" />
		<separator value=";" />
		<separator value="?" />
	</separators>

	<descriptors>

		<descriptor style="reserved" type="WholeWord" description="Set of words to be highlighted" >
			<tokens>
				<token value="abstract"/>
				<token value="event"/>
				<token value="new"/>
				<token value="struct"/>
				<token value="as"/>
				<token value="explicit"/>
				<token value="null"/>
				<token value="switch"/>
				<token value="base"/>
				<token value="extern"/>
				<token value="object"/>
				<token value="this"/>
				<token value="bool"/>
				<token value="false"/>
				<token value="operator"/>
				<token value="throw"/>
				<token value="break"/>
				<token value="finally"/>
				<token value="out"/>
				<token value="true"/>
				<token value="byte"/>
				<token value="fixed"/>
				<token value="override"/>
				<token value="try"/>
				<token value="case"/>
				<token value="float"/>
				<token value="params"/>
				<token value="typeof"/>
				<token value="catch"/>
				<token value="for"/>
				<token value="private"/>
				<token value="uint"/>
				<token value="char"/>
				<token value="foreach"/>
				<token value="protected"/>
				<token value="ulong"/>
				<token value="checked"/>
				<token value="goto"/>
				<token value="public"/>
				<token value="unchecked"/>
				<token value="class"/>
				<token value="if"/>
				<token value="readonly"/>
				<token value="unsafe"/>
				<token value="const"/>
				<token value="implicit"/>
				<token value="ref"/>
				<token value="ushort"/>
				<token value="continue"/>
				<token value="in"/>
				<token value="return"/>
				<token value="using"/>
				<token value="decimal"/>
				<token value="int"/>
				<token value="sbyte"/>
				<token value="virtual"/>
				<token value="default"/>
				<token value="interface"/>
				<token value="sealed"/>
				<token value="volatile"/>
				<token value="delegate"/>
				<token value="internal"/>
				<token value="short"/>
				<token value="void"/>
				<token value="do"/>
				<token value="is"/>
				<token value="sizeof"/>
				<token value="while"/>
				<token value="double"/>
				<token value="lock"/>
				<token value="stackalloc"/>
				<token value="else"/>
				<token value="long"/>
				<token value="static"/>
				<token value="enum"/>
				<token value="namespace"/>
				<token value="string"/>
				<token value="get"/>
				<token value="set"/>
			</tokens>
		</descriptor>

		<descriptor style="quote" type="ToCloseToken" description="String between quotes">
			<tokens>
				<token value="&quot;" closeToken="&quot;" escapeToken="\" />
			</tokens>
		</descriptor>

		<descriptor style="region" type="ToEOL" description="Region block" >
			<tokens>
				<token value="#region"/>
			</tokens>
		</descriptor>

		<descriptor style="region" type="ToEOL" description="Region block" >
			<tokens>
				<token value="#endregion"/>
			</tokens>
		</descriptor>

		<descriptor style="comment" type="ToEOL" description="Comment line" >
			<tokens>
				<token value="//"/>
			</tokens>
		</descriptor>

		<descriptor style="comment" type="ToCloseToken" multiLine="1" description="Comment block">
			<tokens>
				<token value="/\*" closeToken="\*/"/>
			</tokens>
		</descriptor>

	</descriptors>

</highlightDefinition>
