<Nuntiare>
  <Name>Image report</Name>
  <Author>Fredy Ramirez - https://formateli.com</Author>
  <Page>
    <PageWidth>13in</PageWidth>
    <PageHeight>9in</PageHeight>
    <LeftMargin>1cm</LeftMargin>
    <TopMargin>1cm</TopMargin>
    <RightMargin>1cm</RightMargin>
    <BottomMargin>1cm</BottomMargin>
  </Page>
  <Body>
    <ReportItems>
      <Textbox>
        <Name>txt_full_size</Name>
        <Value>Full size embedded image</Value>
        <Height>5mm</Height>
        <Width>5cm</Width>
        <Style>
          <FontSize>16pt</FontSize>
          <PaddingBottom>2mm</PaddingBottom>
        </Style>
      </Textbox>
      <Image>
        <Name>image_full_size</Name>
        <ImageSource>Embedded</ImageSource>
        <MIMEType>image/png</MIMEType>
        <ImageSizing>AutoSize</ImageSizing>
        <Value>formateli</Value>
        <Top>6mm</Top>
        <Height>5cm</Height>
        <Width>5cm</Width>
      </Image>
      <Textbox>
        <Name>txt_fit_size</Name>
        <Value>Fit size embedded image (15cm x 5cm)</Value>
        <Top>2cm</Top>
        <Left>6cm</Left>
        <Height>5mm</Height>
        <Width>12cm</Width>
        <Style>
          <FontSize>16pt</FontSize>
          <PaddingBottom>2mm</PaddingBottom>
        </Style>
      </Textbox>
      <Image>
        <Name>image_fit_size</Name>
        <Top>2.6cm</Top>
        <Left>6cm</Left>
        <ImageSource>Embedded</ImageSource>
        <MIMEType>image/png</MIMEType>
        <ImageSizing>Fit</ImageSizing>
        <Value>formateli</Value>
        <Height>5cm</Height>
        <Width>12cm</Width>
      </Image>
      <Textbox>
        <Name>txt_fit_proportiontal_size_1</Name>
        <Value>Fit proportional size embedded image (15cm x 5cm)</Value>
        <Top>8cm</Top>
        <Left>12cm</Left>
        <Height>5mm</Height>
        <Width>10cm</Width>
        <Style>
          <FontSize>16pt</FontSize>
        </Style>
      </Textbox>
      <Image>
        <Name>image_fitproportional_size_1</Name>
        <ImageSource>Embedded</ImageSource>
        <MIMEType>image/png</MIMEType>
        <ImageSizing>FitProportional</ImageSizing>
        <Value>formateli</Value>
        <Top>8.6cm</Top>
        <Left>12cm</Left>
        <Height>5cm</Height>
        <Width>10cm</Width>
      </Image>
      <Textbox>
        <Name>txt_fit_proportiontal_size_2</Name>
        <Value>Fit proportional size embedded image (5cm x 15cm)</Value>
        <Height>5mm</Height>
        <Width>5cm</Width>
        <Style>
          <FontSize>16pt</FontSize>
          <PaddingBottom>2mm</PaddingBottom>
        </Style>
        <Top>15cm</Top>
        <Left>12cm</Left>
      </Textbox>
      <Image>
        <Name>image_fitproportional_size_2</Name>
        <ImageSource>Embedded</ImageSource>
        <MIMEType>image/png</MIMEType>
        <ImageSizing>FitProportional</ImageSizing>
        <Value>formateli</Value>
        <Top>15.6cm</Top>
        <Left>12cm</Left>
        <Height>10cm</Height>
        <Width>5cm</Width>
      </Image>
      <Textbox>
        <Name>txt_external_fit_proportiontal_size</Name>
        <Value>Fit proportional size external image (10cm x 5cm)</Value>
        <Height>5mm</Height>
        <Width>10cm</Width>
        <Style>
          <FontSize>16pt</FontSize>
          <PaddingBottom>2mm</PaddingBottom>
        </Style>
        <Top>9cm</Top>
      </Textbox>
      <Image>
        <Name>image_external_fitproportional_size</Name>
        <ImageSource>External</ImageSource>
        <MIMEType>image/png</MIMEType>
        <ImageSizing>FitProportional</ImageSizing>
        <Value>formateli.png</Value>
        <Top>9.6cm</Top>
        <Height>10cm</Height>
        <Width>10cm</Width>
      </Image>
    </ReportItems>
    <Style/>
  </Body>
  <EmbeddedImages>
    <EmbeddedImage>
      <Name>formateli</Name>
      <MIMEType>image/png</MIMEType>
      <ImageData></ImageData>
    </EmbeddedImage>
  </EmbeddedImages>
</Nuntiare>
