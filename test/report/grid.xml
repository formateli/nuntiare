<?xml version="1.0" encoding="UTF-8"?>
<Report>
    <Name>Grid Test</Name>
    <Description>
        A Grid Example. 
        Note that Tablix has no DataSet assigned, 
        neither TablixColumnHierarchy nor TablixRowHierarchy, 
        so it is a Grid.
    </Description>
    <Author>Fredy Ramirez</Author>
    <Width>8.5in</Width>

    <ReportParameters>
        <ReportParameter>
            <Name>parameter0</Name>
            <DataType>Integer</DataType>
            <CanBeNone>false</CanBeNone>
            <AllowBlank>false</AllowBlank>
            <DefaultValue>3000</DefaultValue>
        </ReportParameter>
        <ReportParameter>
            <Name>parameter1</Name>
            <DataType>Integer</DataType>
            <CanBeNone>false</CanBeNone>
            <AllowBlank>false</AllowBlank>
            <DefaultValue>=Parameters['parameter0'] + 2000</DefaultValue>
        </ReportParameter>
    </ReportParameters>

    <Page>
        <PageWidth>210mm</PageWidth>
        <PageHeight>1000mm</PageHeight>
        <TopMargin>1cm</TopMargin>
        <LeftMargin>1cm</LeftMargin>
        <RightMargin>1cm</RightMargin>
        <BottomMargin>1cm</BottomMargin>
        <PageHeader>
            <Height>1.5cm</Height>
            <PrintOnFirstPage>true</PrintOnFirstPage>
            <PrintOnLastPage>true</PrintOnLastPage>
            <Style>
                <BottomBorder>
                    <Style>Solid</Style>
                    <Color>Black</Color>
                    <Width>2pt</Width>
                </BottomBorder>
            </Style>
            <ReportItems>
                <Textbox>
                    <Name>header1</Name>
                    <Height>1.5cm</Height>
                    <Width>140mm</Width>
                    <Value>Grid Test</Value>
                    <Left>0.00pt</Left>
                    <Top>3mm</Top>
                    <Style>
                        <FontWeight>Bold</FontWeight>
                        <FontSize>16pt</FontSize>
                        <TextAlign>Center</TextAlign>
                        <VerticalAlign>Middle</VerticalAlign>
                    </Style>
                    <ZIndex>1</ZIndex>
                </Textbox>
            </ReportItems>
        </PageHeader>        

        <PageFooter>
            <Height>1.5cm</Height>
            <PrintOnFirstPage>true</PrintOnFirstPage>
            <PrintOnLastPage>true</PrintOnLastPage>
            <Style>
                <TopBorder>
                    <Color>Black</Color>
                    <Width>2 pt</Width>
                    <Style>Solid</Style>
                </TopBorder>
            </Style>
            <ReportItems>
                <Textbox>
                    <Name>reportname</Name>
                    <Height>8mm</Height>
                    <Width>60mm</Width>
                    <Value>='Report name: ' + Globals['report_name']</Value>
                    <Left>0mm</Left>
                    <Top>2mm</Top>
                    <ZIndex>0</ZIndex>
                </Textbox>
                <Textbox>
                    <Name>reportfolder</Name>
                    <Height>8mm</Height>
                    <Width>60mm</Width>
                    <Value>='Folder: ' + Globals['report_folder']</Value>
                    <Left>0mm</Left>
                    <Top>12mm</Top>
                    <ZIndex>1</ZIndex>
                </Textbox>
                <Textbox>
                    <Name>reportfile</Name>
                    <Height>8mm</Height>
                    <Width>60mm</Width>
                    <Value>='File: ' + Globals['report_file']</Value>
                    <Left>0mm</Left>
                    <Top>22mm</Top>
                    <ZIndex>1</ZIndex>
                </Textbox>
                <Textbox>
                    <Name>reporttime</Name>
                    <Height>8mm</Height>
                    <Width>90mm</Width>
                    <Value>=Globals['execution_time']</Value>
                    <Left>100mm</Left>
                    <Top>2mm</Top>
                    <ZIndex>2</ZIndex>
                    <Style>
                        <Format>{:%A, %m-%d-%Y %H:%M:%S}</Format>
                        <TextAlign>Right</TextAlign>
                    </Style>
                </Textbox>
                <Textbox>
                    <Name>reportpage</Name>
                    <Height>8mm</Height>
                    <Width>90mm</Width>
                    <Value>='Page ' + str(Globals['page_number']) + ' of ' + str(Globals['total_pages'])</Value>
                    <Left>100mm</Left>
                    <Top>12mm</Top>
                    <ZIndex>2</ZIndex>
                    <Style>
                        <TextAlign>Right</TextAlign>
                    </Style>
                </Textbox>
            </ReportItems>
        </PageFooter>
    </Page>
  
